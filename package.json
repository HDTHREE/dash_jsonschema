{
  "name": "dash_jsonschema",
  "version": "1.0.0",
  "description": "A dash wrapper for https://github.com/rjsf-team/react-jsonschema-form.",
  "main": "index.ts",
  "scripts": {
    "build:js::dev": "webpack --mode development",
    "build:js": "webpack",
    "build:backends": "dash-generate-components ./src/ts/components dash_jsonschema -p package-info.json --r-prefix 'djsf' --jl-prefix 'djsf' --ignore \\.test\\.",
    "build": "npm run build:js && npm run build:backends",
    "watch": "npm run build:js::dev -- --watch",
    "build:venv": "python3 -m venv venv && venv/bin/pip install -r requirements.txt && echo '\n\nsource venv/bin/activate'"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "css-loader": "^6.7.1",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.28.0",
    "react": "^16.14.0",
    "react-docgen": "^5.4.0",
    "react-dom": "^16.14.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "author": "Hayden <hd3nnis4@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@rjsf/core": "^5.24.13",
    "@rjsf/utils": "^5.24.13",
    "@rjsf/validator-ajv8": "^5.24.13"
  }
}
